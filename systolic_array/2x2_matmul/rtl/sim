    %vpi_call 2 88 "$display", "C22 = %0d  (Expected: 50)", v0x587e7e6e09d0_0 {0 0 0};
    %vpi_call 2 89 "$display", "========================================" {0 0 0};
    %load/vec4 v0x587e7e6e0650_0;
    %pad/s 32;
    %pushi/vec4 19, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x587e7e6e07b0_0;
    %pad/s 32;
    %pushi/vec4 22, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %load/vec4 v0x587e7e6e08c0_0;
    %pad/s 32;
    %pushi/vec4 43, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %load/vec4 v0x587e7e6e09d0_0;
    %pad/s 32;
    %pushi/vec4 50, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_6.2, 8;
    %vpi_call 2 92 "$display", "PASSED!" {0 0 0};
    %jmp T_6.3;
T_6.2 ;
    %vpi_call 2 94 "$display", "FAILED!" {0 0 0};
T_6.3 ;
    %vpi_call 2 96 "$display", "========================================\012" {0 0 0};
    %delay 20000, 0;
    %vpi_call 2 99 "$finish" {0 0 0};
    %end;
    .thread T_6;
